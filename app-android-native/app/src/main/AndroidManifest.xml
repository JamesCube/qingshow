        </activity>
        <activity
            android:name="com.focosee.qingshow.activity.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.focosee.qingshow.activity.PersonalActivity"
            android:label="@string/title_activity_personal" >
        </activity>
        <activity
            android:name="com.focosee.qingshow.activity.S01HomeActivity"
            android:label="@string/title_activity_s01_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

