<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/RelativeLayout_height_activity_model"
    android:layout_marginBottom="4dp">
    <com.facebook.drawee.view.SimpleDraweeView
        android:background="@color/white"
        android:id="@+id/s13_head_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="9"
        fresco:placeholderImage="@drawable/bg_pic" />

    <ImageButton
        android:id="@+id/s13_head_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/s03_back_btn"/>


</RelativeLayout>
