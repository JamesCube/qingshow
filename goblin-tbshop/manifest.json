{
    "manifest_version": 2,
    "name": "ChingShow Crawler",
    "version": "1.1.0",
    "description": "",
    "permissions": [],
    "icons": {
        "16": "/images/goblin.png",
        "48": "/images/goblin.png",
        "128": "/images/goblin.png"
    },
    "background": {
        "scripts": ["/libs/underscore-1.7.0/underscore.js",
            "/libs/async-0.9.0/lib/async.js",
            "/background/background.js"],
        "persistent": false
    },
    "page_action": {
        "default_icon": "/images/goblin.png"
    },
    "content_scripts": [
        {
            "matches": ["http://nz.taobao.com/*"],
            "js": ["/libs/jquery-2.1.1/jquery-2.1.1.js",
                "/libs/underscore-1.7.0/underscore.js",
                "/libs/async-0.9.0/lib/async.js",
                "/libs/URI.js-1.14.1/src/URI.js",
                "/contentScripts/RuntimeAPI.js",
                "/contentScripts/taobao.com/nz.js"]
        },
        {
            "matches": ["http://www.taobao.com/market/nvzhuang/*"],
            "js": ["/libs/jquery-2.1.1/jquery-2.1.1.js",
                "/libs/underscore-1.7.0/underscore.js",
                "/libs/async-0.9.0/lib/async.js",
                "/libs/URI.js-1.14.1/src/URI.js",
                "/contentScripts/RuntimeAPI.js",
                "/contentScripts/taobao.com/www/market/nvzhuang.js"]
        },
        {
            "matches": ["http://s.taobao.com/list/*"],
            "js": ["/libs/jquery-2.1.1/jquery-2.1.1.js",
                "/libs/underscore-1.7.0/underscore.js",
                "/libs/async-0.9.0/lib/async.js",
                "/libs/URI.js-1.14.1/src/URI.js",
                "/contentScripts/RuntimeAPI.js",
                "/contentScripts/taobao.com/s/list.js"]
        },
        {
            "matches": ["http://*.taobao.com/shop/view_shop.htm*"],
            "js": ["/libs/jquery-2.1.1/jquery-2.1.1.js",
                "/libs/underscore-1.7.0/underscore.js",
                "/libs/async-0.9.0/lib/async.js",
                "/libs/URI.js-1.14.1/src/URI.js",
                "/contentScripts/RuntimeAPI.js",
                "/contentScripts/taobao.com/$shop/shop/view_shop.js"]
        },
        {
            "matches": ["http://*.taobao.com/search.htm*"],
            "js": ["/libs/jquery-2.1.1/jquery-2.1.1.js",
                "/libs/underscore-1.7.0/underscore.js",
                "/libs/async-0.9.0/lib/async.js",
                "/libs/URI.js-1.14.1/src/URI.js",
                "/contentScripts/RuntimeAPI.js",
                "/contentScripts/taobao.com/$shop/search.js"]
        }
    ]
}