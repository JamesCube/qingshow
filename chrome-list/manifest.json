{
    "name": "倾秀上货插件",
    "version": "1.0",
    "permissions": [
        "http://dev.chingshow.com/*",
        "http://chingshow.com/*"
    ],
    "page_action": {
        "default_title": "上货"
    },
    "background": {
        "scripts": [
            "/background/background.js"
        ]
    },
    "icons": {
        "16": "/images/icon.png",
        "32": "/images/icon.png",
        "48": "/images/icon.png",
        "128": "/images/icon.png"
    },

    "content_scripts" : [{
        "matches": [
            "*://item.taobao.com/*",
            "*://*.tmall.com/item.*"
        ],
        "js": [
            "/libs/jquery-2.1.1/jquery-2.1.1.js",
            "/libs/underscore-1.7.0/underscore.js",
            "/libs/async-0.9.0/lib/async.js",
            "/libs/URI.js-1.14.1/src/URI.js",
            "/contentScripts/RuntimeAPI.js"
        ]
    }],
    "manifest_version": 2
}
